package client;

public class TestClass {

	public static void main(String[] args) {
		System.out.println(Math.round(4.3d));
		System.out.println(Math.round(4.5d));
		System.out.println(Math.round(4.6d));

		System.out.println((int)Math.ceil(4.3d));
		System.out.println((int)Math.ceil(4.5d));
		System.out.println((int)Math.ceil(4.6d));

	}

}
