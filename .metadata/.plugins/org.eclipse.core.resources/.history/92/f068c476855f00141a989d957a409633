 */

public class Server  {

	private List<POP3Account> input = new ArrayList<POP3Account>();
	public static void main(String[] args) {
		// Parameter: int = Maximale Anzahl Clients
		
		
		
		POP3Proxy server = new POP3Proxy(15, input);
		server.start();
	}

}
